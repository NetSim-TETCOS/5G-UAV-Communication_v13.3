						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="NETWORK_CONFIGURATION" maxOccurs="1" minOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="DEVICE_CONFIGURATION" minOccurs="1" maxOccurs="1">
								<xs:complexType>
									<xs:sequence>
										<xs:element maxOccurs="unbounded" name="DEVICE">
											<xs:complexType>
												<xs:sequence>
													<xs:choice>
														<xs:element name="POS_3D" minOccurs="0" maxOccurs="1">
															<xs:complexType>
																<xs:sequence>
																	<xs:element name="NODE_JOIN_PROFILE" maxOccurs="1" minOccurs="0">
																		<xs:complexType>
																			<xs:attribute name="JOIN_AT" type="xs:string" use="required" />
																			<xs:attribute name="LEAVE_AT" type="xs:string" use="optional" />
																		</xs:complexType>
																	</xs:element>
																	<xs:element name="MOBILITY" maxOccurs="1" minOccurs="0">
																		<xs:complexType>
																			<xs:attribute name="MODEL" type="MobilityModel" use="required" />
																			<xs:attribute name="VELOCITY" type="xs:integer" use="optional" />
																			<xs:attribute name="PAUSE_TIME" type="xs:integer" use="optional" />
																			<xs:attribute name="CALCULATION_INTERVAL" type="xs:double" use="optional"/>
																			<xs:attribute name="GROUP_ID" type="xs:positiveInteger" use="optional"/>
																			<xs:attribute name="FILE_NAME" type="xs:string" use="optional"/>
																			<xs:attribute name="STEP_SIZE" type="xs:string" use="optional"/>
																			<xs:attribute name="MAX_SPEED_M_S" type="xs:double" use="optional" />
																			<xs:attribute name="MIN_SPEED_M_S" type="xs:double" use="optional" />
																			<xs:attribute name="STOP_PROBABILITY" type="xs:double" use="optional" />
																			<xs:attribute name="STOP_DURATION_S" type="xs:integer" use="optional" />
																			<xs:attribute name="OPEN_MOBILITY_FILE" type="xs:string" use="optional" />
																		</xs:complexType>
																	</xs:element>
																</xs:sequence>
																<xs:attribute name="X-POS" type="xs:double" use="optional" />
																<xs:attribute name="Y-POS" type="xs:double" use="optional" />
																<xs:attribute name="Z-POS" type="xs:double" use="optional" />
																<xs:attribute name="X_OR_LON" type="xs:double" use="optional" />
																<xs:attribute name="Y_OR_LAT" type="xs:double" use="optional" />
																<xs:attribute name="Z" type="xs:double" use="optional" />
															</xs:complexType>
														</xs:element>
														<xs:element name="POS_MAP" minOccurs="0" maxOccurs="1">
															<xs:complexType>
																<xs:sequence>
																	<xs:element name="NODE_JOIN_PROFILE" maxOccurs="1" minOccurs="0">
																		<xs:complexType>
																			<xs:attribute name="JOIN_AT" type="xs:string" use="required" />
																			<xs:attribute name="LEAVE_AT" type="xs:string" use="optional" />
																		</xs:complexType>
																	</xs:element>
																	<xs:element name="MOBILITY" maxOccurs="1" minOccurs="0">
																		<xs:complexType>
																			<xs:attribute name="MODEL" type="MobilityModel" use="required" />
																			<xs:attribute name="VELOCITY" type="xs:integer" use="optional" />
																			<xs:attribute name="PAUSE_TIME" type="xs:integer" use="optional" />
																			<xs:attribute name="CALCULATION_INTERVAL" type="xs:double" use="optional"/>
																			<xs:attribute name="GROUP_ID" type="xs:positiveInteger" use="optional"/>
																			<xs:attribute name="FILE_NAME" type="xs:string" use="optional"/>
																			<xs:attribute name="STEP_SIZE" type="xs:string" use="optional"/>
																			<xs:attribute name="MAX_SPEED_M_S" type="xs:double" use="optional" />
																			<xs:attribute name="MIN_SPEED_M_S" type="xs:double" use="optional" />
																			<xs:attribute name="STOP_PROBABILITY" type="xs:double" use="optional" />
																			<xs:attribute name="STOP_DURATION_S" type="xs:integer" use="optional" />
																		</xs:complexType>
																	</xs:element>
																</xs:sequence>
																<xs:attribute name="X_OR_LON" type="xs:double" use="required" />
																<xs:attribute name="Y_OR_LAT" type="xs:double" use="required" />
																<xs:attribute name="Z" type="xs:double" use="required" />
