	<xs:element name="PROTOCOL_PROPERTY">
		<!-- ADD ELEMENT FOR PROTOCOL PROPERTY-->
		<xs:complexType>
			<xs:sequence>
				<xs:element name="INCUMBENT" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:attributeGroup ref="CR_INCUMBENT_PROPERTY" />
					</xs:complexType>
				</xs:element>

				<xs:element name="AC_BK" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:attributeGroup ref="WLAN_AC_BK" />
					</xs:complexType>
				</xs:element>

				<xs:element name="AC_BE" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:attributeGroup ref="WLAN_AC_BE" />
					</xs:complexType>
				</xs:element>

				<xs:element name="AC_VI" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:attributeGroup ref="WLAN_AC_VI" />
					</xs:complexType>
				</xs:element>

				<xs:element name="AC_VO" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:attributeGroup ref="WLAN_AC_VO" />
					</xs:complexType>
				</xs:element>

				<!-- IEEE802.15.4 PHY and MAC PROPERTY-->
				<xs:element name="POWER" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:attributeGroup ref="WSN_POWER" />
