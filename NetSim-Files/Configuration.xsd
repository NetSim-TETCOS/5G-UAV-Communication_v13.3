					</xs:complexType>
				</xs:element>
				<xs:element name="AGENT" minOccurs="0" maxOccurs="100">
					<xs:complexType>
						<xs:attributeGroup ref="WSN_AGENT" />
					</xs:complexType>
				</xs:element>
				<xs:element name="SINK_NODE" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:attributeGroup ref="WSN_SINKNODE" />
					</xs:complexType>
				</xs:element>
				<!-- <xs:element name="CA" minOccurs="0" maxOccurs="5">
					<xs:complexType>
						<xs:attributeGroup ref ="LTE-A_CA"/>
					</xs:complexType>
				</xs:element> -->

				<xs:group ref="LTE_NR_ELEMENT"/>


				<xs:group ref ="SATELLITE_PHY_ELEMENT"/>
				<xs:element ref ="PROTOCOL_PROPERTY" minOccurs="0" maxOccurs="1"/>
			</xs:sequence>
			<!-- ETHERNET PHY PROPERTY-->
			<xs:attribute name="ETHERNET_STANDARD" type="xs:string" use="optional" />
			<xs:attribute name="CONNECTION_MODE" use="optional" type="Connection_Mode" />
			<xs:attribute name="LINK_TYPE" type="MEDIUM" use="optional" />
			<xs:attribute name="CONNECTED_TO" use="optional"/>
			<xs:attribute name="MAC_ADDRESS" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:pattern value="[0-9a-fA-F]+" />
						<xs:length value="12" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ISNSAENABLE" type="xs:string" use="optional" />
			<xs:attribute name="SELECTED_OPTION_TYPE" type="xs:string" use="optional" />
			<xs:attribute name="MASTER_NODE_TYPE" type="xs:string" use="optional" />
			<xs:attribute name="SECONDERY_NODE_TYPE" type="xs:string" use="optional" />
			<!-- ETHERNET MAC PROPERTY-->
			<xs:attribute name ="IGMP_SNOOPING" type="BOOL" use="optional"/>
			<xs:attribute name="VLAN_STATUS" type="BOOL" use="optional"/>
			<xs:attribute name="VLAN_NAME" type="xs:string" use="optional" />
			<xs:attribute name="VLAN_GUI" type="xs:string" use="optional" />
			<xs:attribute name ="VLAN_ID" type="xs:positiveInteger" use="optional"/>
			<xs:attribute name="VLAN_PORT_TYPE" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="ACCESS_PORT"/>
						<xs:enumeration value="TRUNK_PORT"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ICMP_STATUS" type="BOOL" use="optional"/>
			<xs:attribute name="PROMISCUOUS_MODE" type="BOOL" use="optional"/>
			<xs:attribute name ="SPT_STATUS" type="BOOL" use="optional"/>
			<xs:attribute name="BUFFER_SIZE" type="xs:float" use="optional" />
			<xs:attribute name="SWITCH_ID" type="xs:string" use="optional" />
			<xs:attribute name="SWITCHING_TECHNIQUE" type="xs:string" use="optional" />
			<xs:attribute name="SPANNING_TREE_PROTOCOL" type="xs:string" use="optional" />
			<xs:attribute name="SPEED" type="xs:string" use="optional" />
			<xs:attribute name="SWITCHING_LATENCY" type="xs:float" use="optional" />
			<xs:attribute name="COST" type="xs:integer" use="optional" />
			<xs:attribute name="SWITCH_PRIORITY" type="xs:positiveInteger" use="optional" />
			<xs:attribute name="PERSISTANCE" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:pattern value="1/[1-9]"/>
						<xs:pattern value="1/1[0-5]"/>
						<xs:pattern value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ERROR_RATE" use="optional" type="xs:double"/>
			<!--Common Mac and Phy Property-->
			<xs:attributeGroup ref="COMMON_MAC_PHY_PROPERTY"/>
			<!--ARP PROPERTY-->
			<xs:attribute name="ARP_RETRY_INTERVAL" type="xs:positiveInteger" use="optional" />
			<xs:attribute name="ARP_RETRY_LIMIT" type="xs:positiveInteger" use="optional" />

			<!--IEEE802.11 Property-->
			<xs:attributeGroup ref="IEEE802_11_PROPERTY"/>
			<!--IEEE802.22 Property-->
			<xs:attributeGroup ref="CR_PROPERTY" />
			<!-- IEEE802.15.4 PHY and MAC PROPERTY-->
			<xs:attributeGroup ref="WSN_PROPERTY" />
			<!--CELLULAR PROPERTY-->
			<xs:attributeGroup ref="CELLULAR_PROPERTY" />
			<!--LTE Property-->
			<xs:attributeGroup ref="LTE_PROPERTY"/>
			<xs:attributeGroup ref="ADVANCE_LTE_PROPERTY"/>
			<!--LTE-NR Property-->
			<xs:attributeGroup ref="LTE_NR_PROPERTY"/>
			<!--WIMAX Property-->
			<xs:attributeGroup ref="WIMAX_PROPERTY"/>
			<!-- TDMA property-->
			<xs:attributeGroup ref="TDMA_PROPERTY"/>
			<xs:attributeGroup ref ="DTDMA_PROPERTY"/>
			<!-- Aloha property-->
			<xs:attributeGroup ref="ALOHA_PROPERTY"/>
			<!--VANET Property-->
			<xs:attributeGroup ref="VANET_PROPERTY"/>
			<!--SATELLITE Property-->
			<xs:attributeGroup ref="SATELLITE_MAC_PROPERTY"/>
			<xs:attributeGroup ref="SATELLITE_PHY_PROPERTY"/>
		</xs:complexType>
	</xs:element>
